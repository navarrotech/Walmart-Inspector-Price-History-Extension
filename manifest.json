{
  "name": "Walmart insight: The leading price tracker & website addons",
  "version": "2025.2.3",
  "manifest_version": 3,
  "description": "Adds a price tracker, website history addons, and more to Walmart.com",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "64": "icons/icon64.png",
      "96": "icons/icon96.png",
      "128": "icons/icon128.png",
      "256": "icons/icon256.png"
    },
    "default_title": "Walmart Insight"
  },
  "background": {
    "service_worker": "dist/walmart-inspector-background.js"
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "dist/themes/dark.css",
        "dist/themes/bubblegum.css",
        "dist/themes/spark.css"
      ],
      "matches": [
        "https://*.walmart.com/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.walmart.com/**"
      ],
      "js": [
        "dist/contentScript.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "walmart-insight@navarrotech",
      "strict_min_version": "109.0"
    }
  }
}